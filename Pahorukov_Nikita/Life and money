#include <stdio.h>
const double inflat =0.15; // инфляция (годовая)
const double bank_prots = 1.13; // банковский процент (годовой)
const double nds = 0.13;
double bank_schet = 0;
int food = 0;

void my_food(int month)
{
	if (1 <= month and month <= 6) 
		food = 20000;
	else if (month <= 12) 
		food = 15000;
}

void my_bank_schet(int month) {
	if (month == 12)
		bank_schet *= bank_prots;
}

void inflation() {
	bank_schet -= bank_schet * inflat;
}

long int nalogi(long int salary) {
	return salary - salary * nds;
}
void my_cat(int month) {
	bank_schet -= 5000;
}
int main()
{
	long int salary = 150'000 *(1 - nds);
	int month = 1;
	int year = 2026;
	while (!(year == 2027 and month == 2)) {
		if (month == 13) {
			++year;
			month = 1;
			inflation();
		}
		if (month == 7) {
			bank_schet -= 200'000 + salary;

		}
		my_food(month);
		bank_schet += salary;
		bank_schet -= food;
		my_bank_schet(month);
		my_cat(month);
		if (bank_schet <= 0) {
			printf("ti bankrot ahahah");
		}
		printf("%d %d %f\n",month, salary, bank_schet);
		++month;
		
	}
	
}
